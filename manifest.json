{
    "manifest_version": 3,
    "name": "AI Web Reader",
    "version": "1.0",
    "description": "An extension to read or summarize web pages with AI voice options",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ]
  }
  